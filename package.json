{
  "name": "deno.ns",
  "version": "0.4.4",
  "description": "Deno namespace shim for Node.js",
  "keywords": [
    "deno namespace",
    "deno",
    "polyfill",
    "ponyfill",
    "shim"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Thomas Rory Gummerson <thomas@gummerson.no> (https://trgwii.no/)",
  "contributors": [
    "Wojciech Pawlik <woj.pawlik@gmail.com>",
    "Muthu Kumar <muthukumar@thefeathers.in> (https://mkr.pw)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/denoland/deno.ns.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./global": "./dist/global.js",
    "./deno": "./dist/deno.js"
  },
  "files": [
    "dist",
    "src/deno/stable/lib.deno.d.ts"
  ],
  "scripts": {
    "denolib": "deno run --allow-run --allow-write=src/deno/ tools/denolib.ts",
    "build": "tsc",
    "prepare": "npm run --silent denolib && npm run --silent build",
    "lint": "deno lint --ignore=node_modules,dist,thirdparty,unit,src/deno/stable/lib.deno.d.ts",
    "fmt": "deno fmt --ignore=node_modules,dist,thirdparty,unit,src/deno/stable/lib.deno.d.ts",
    "clean": "git clean -fXde !node_modules/",
    "test": "NODE_OPTIONS=--experimental-loader=ts-node/esm fdt --filter=$(node tools/skip_tests.cjs) tools/setup_tests.mjs $(cat tools/working_test_files.txt)"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "dependencies": {
    "undici": "^4.5.1"
  },
  "peerDependencies": {
    "@fromdeno/test": "<0.3.0"
  },
  "devDependencies": {
    "@fromdeno/test": "^0.1.5",
    "@types/node": "^16.4.13",
    "conditional-type-checks": "^1.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
